{
  "name": "gitbook-plugin-citation-bibtex",
  "description": "Bibtex citations for GitBook",
  "author": "Guillermo Blanco",
  "main": "index.js",
  "version": "0.0.2",
  "engines": {
    "gitbook": "*"
  },
  "gitbook": {
    "properties": {
      "path": {
        "type": "string",
        "description": "path to literature file",
        "required": false,
        "default": "./literature.bib"
      }
    }
  },
  "homepage": "https://github.com/m0n0l0c0/gitbook-plugin-index-of-figures.git",
  "repository": {
    "type": "git",
    "url": "https://github.com/m0n0l0c0/gitbook-plugin-index-of-figures.git"
  },
  "license": "MIT",
  "dependencies": {
    "bib2json": "0.0.1"
  },
  "scripts": {}
}
